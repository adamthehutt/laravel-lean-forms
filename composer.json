{
    "name": "adamthehutt/laravel-lean-forms",
    "description": "Code-based form configuration for Laravel. Lean, clean, and mean blade templates.",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=5.8",
        "illuminate/database": ">=5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "8.2.1",
        "orchestra/testbench": "^3.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Huttler",
            "email": "adam.huttler@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AdamTheHutt\\LeanForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdamTheHutt\\LeanForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdamTheHutt\\LeanForms\\LeanFormsServiceProvider"
            ]
        }
    }
}
